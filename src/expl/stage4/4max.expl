decl
	int a,b,c,d;
	int max;
enddecl;
begin
	read(a);
	read(b);
	read(c);
	read(d);

	if(a >b) then
		if( a > c) then
			if(a > d) then
				max = a;
			else
				max =d;
			endif;
		else
			if(c > d) then
				max = c;
			else
				max = d;
			endif;
		endif;
	else
		if( b> c) then
			if(b > d) then
				max = b;
			else
				max = d;
			endif;
		else
			if(c >d) then
				max =c;
